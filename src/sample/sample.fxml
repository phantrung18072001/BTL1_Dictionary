<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="507.0" prefWidth="590.0" stylesheets="@View.css" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints vgrow="NEVER" />
   </rowConstraints>
   <Pane fx:id="SEARCH" prefHeight="538.0" prefWidth="590.0">
      <Pane layoutX="-4.0" layoutY="2.0" prefHeight="535.0" prefWidth="599.0" style="-fx-background-color: #E0F8F1;" styleClass="background" stylesheets="@View.css">
         <children>
            <TextField fx:id="textSearch" layoutX="41.0" layoutY="68.0" prefHeight="31.0" prefWidth="160.0" promptText="Search word..." styleClass="text_search" stylesheets="@View.css">
               <font>
                  <Font size="15.0" />
               </font></TextField>
            <ListView fx:id="listView" layoutX="41.0" layoutY="105.0" prefHeight="367.0" prefWidth="246.0" stylesheets="@View.css" />
            <TextArea fx:id="textExplain" layoutX="320.0" layoutY="105.0" prefHeight="367.0" prefWidth="228.0" stylesheets="@View.css">
               <font>
                  <Font size="15.0" />
               </font></TextArea>
            <Button fx:id="sound" layoutX="252.0" layoutY="67.0" mnemonicParsing="false" onAction="#sound" prefHeight="32.0" prefWidth="35.0" styleClass="sound" stylesheets="@View.css">
               <graphic>
                  <ImageView fitHeight="21.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/icons8_Speaker_48px.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="butAdd" layoutX="417.0" layoutY="69.0" mnemonicParsing="false" onAction="#add" prefHeight="30.0" prefWidth="32.0" styleClass="add" stylesheets="@View.css">
               <graphic>
                  <ImageView fitHeight="21.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/icons8_Plus_Math_52px.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="butDel" layoutX="516.0" layoutY="69.0" mnemonicParsing="false" onAction="#deleteWord" prefHeight="30.0" prefWidth="32.0" styleClass="delete" stylesheets="@View.css">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/icons8_Trash_52px.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="butFind" layoutX="207.0" layoutY="67.0" mnemonicParsing="false" onAction="#search" prefHeight="30.0" prefWidth="32.0" styleClass="search" stylesheets="@View.css">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="21.0" pickOnBounds="true">
                     <image>
                        <Image url="@../Images/icons8_Search_48px.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button fx:id="butUpdate" layoutX="466.0" layoutY="69.0" mnemonicParsing="false" onAction="#update" prefHeight="30.0" prefWidth="35.0" styleClass="change" stylesheets="@View.css">
               <graphic>
                  <ImageView fitHeight="19.0" fitWidth="73.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/icons8_Edit_52px_2.jpg" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
      </Pane>
      <Pane fx:id="topbar" layoutX="-11.0" prefHeight="57.0" prefWidth="772.0" style="-fx-background-color: #3D4956;" stylesheets="@View.css">
         <children>
            <Label layoutX="221.0" layoutY="10.0" prefHeight="37.0" prefWidth="143.0" text="Dictionary" textFill="#eee8e8">
               <font>
                  <Font name="SansSerif Bold" size="26.0" />
               </font>
            </Label>
            <ImageView fitHeight="57.0" fitWidth="63.0" layoutX="47.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/images.jpg" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </Pane>
</GridPane>
